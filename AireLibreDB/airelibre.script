INSERT INTO ACTIVIDAD VALUES('Tenis Paral\u00edmpico',2700.0E0,'Tenis Paral\u00edmpico es una actividad inclusiva dise\u00f1ada para deportistas con discapacidad que buscan desarrollar sus habilidades en el tenis.','aced00057372000d6a6176612e74696d652e536572955d84ba1b2248b20c00007870770d010000000000000e100000000078','Aceptada','2024-11-12',16,'RF')
